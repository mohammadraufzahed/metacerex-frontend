{
  "name": "saraphi-ui",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "build:tailwind": "tailwindcss -m -i ./src/styles/tailwind.scss -o ./src/styles/tailwind.css",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr ./src/entry-server.tsx --outDir dist/server",
    "build": "npm run build:tailwind && npm run build:server && npm run build:client",
    "generate": "vite build --outDir /dist/static",
    "serve": "cross-env NODE_ENV=production node server",
    "dev:tailwind": "tailwindcss -w -i ./src/styles/tailwind.scss -o ./src/styles/tailwind.css",
    "dev": "concurrently \"npm run dev:tailwind\" \"node server\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "targets": {
    "server": {
      "context": "node",
      "includeNodeModules": true
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.5",
    "@vitejs/plugin-react": "^2.0.1",
    "autoprefixer": "^10.4.8",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.16",
    "sass": "^1.54.4",
    "tailwindcss": "^3.1.8",
    "typecript": "0.0.1-security",
    "typescript": "^4.7.4",
    "vite": "^3.0.6"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.18.1",
    "framer-motion": "^7.1.0",
    "hamburger-react": "^2.5.0",
    "localforage": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.3.0",
    "react-tradingview-embed": "^3.0.6",
    "recoil": "^0.7.5",
    "recoil-nexus": "^0.4.0",
    "serve-static": "^1.15.0",
    "vite-plugin-svgr": "^2.2.1"
  }
}
